<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class
		name="com.mtsmda.hibenateOnly.hibenateOnly.model.CompositeKeyWithinEntityXML"
		table="COMPOSITE_KEY_WITHIN_ENTITY_XML" lazy="false">
		<composite-id>
			<key-property name="id" type="int" column="ID" />
			<key-property name="departamentId" type="int" column="DEPARTAMENTID" />
		</composite-id>
		<property name="name">
			<column name="NAME" sql-type="varchar(255)" not-null="true" />
		</property>
	</class>
</hibernate-mapping>